{
  "name": "@soubhikchatterjee/react-pdf",
  "version": "0.1.19",
  "author": "Soubhik Chatterjee <soubhik@chatterjee.pw>",
  "contributors": [
    {
      "name": "Soubhik Chatterjee",
      "email": "soubhik@chatterjee.pw",
      "url": "https://soubhik.chatterjee.pw"
    }
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-optional-chaining"
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soubhikchatterjee/react-pdf.git"
  },
  "bugs": {
    "url": "https://github.com/soubhikchatterjee/react-pdf/issues"
  },
  "keywords": [
    "pdf viewer",
    "zoom pdf page",
    "rotate pdf page",
    "scroll pdf pages",
    "rearrange pdf pages"
  ],
  "peerDependencies": {
    "classnames": "^2.2.6",
    "pdfjs-dist": "^2.6.347",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.3",
    "react-tooltip": "^4.2.15",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "NODE_ENV=production react-scripts build",
    "build": "rm -rf dist && mkdir dist &&  babel src -d dist --copy-files",
    "publish:npm": "npm publish --access public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.13.0",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "babel-preset-react-app": "^10.0.0",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "react-tooltip": "^4.2.15"
  }
}
